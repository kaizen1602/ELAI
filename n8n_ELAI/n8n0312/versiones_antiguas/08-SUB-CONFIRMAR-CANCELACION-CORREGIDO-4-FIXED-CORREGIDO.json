        "functionCode": "// Validar inputs\nconst citaId = Number($json.cita_id);\nconst pacienteId = Number($json.paciente_id);\nconst token = $json.token;\n\nconsole.log('=== VALIDAR INPUTS ===');\nconsole.log('cita_id:', citaId);\nconsole.log('paciente_id:', pacienteId);\nconsole.log('token presente:', !!token);\n\nif (!citaId || isNaN(citaId) || citaId <= 0) {\n    return {\n        json: {\n            success: false,\n            mensaje: \"ID de cita inválido\",\n            valid: false\n        }\n    };\n}\n\nif (!pacienteId || isNaN(pacienteId) || pacienteId <= 0) {\n    return {\n        json: {\n            success: false,\n            mensaje: \"ID de paciente inválido\",\n            valid: false\n        }\n    };\n}\n\nif (!token) {\n    return {\n        json: {\n            success: false,\n            mensaje: \"Token JWT requerido\",\n            valid: false\n        }\n    };\n}\n\nreturn {\n    json: {\n        cita_id: citaId,\n        paciente_id: pacienteId,\n        token: token,\n        valid: true\n    }\n};"
      },
