        "functionCode": "// Validar que el paciente_id sea válido\nconst pacienteId = Number($json.paciente_id);\nconst token = $json.token;\n\nconsole.log('=== VALIDAR INPUT ===');\nconsole.log('paciente_id:', pacienteId);\nconsole.log('token presente:', !!token);\n\nif (!pacienteId || isNaN(pacienteId) || pacienteId <= 0) {\n    return {\n        json: {\n            success: false,\n            mensaje: \"ID de paciente inválido o no proporcionado\",\n            total_citas: 0,\n            citas: []\n        }\n    };\n}\n\nif (!token) {\n    return {\n        json: {\n            success: false,\n            mensaje: \"Token JWT es requerido\",\n            total_citas: 0,\n            citas: []\n        }\n    };\n}\n\nreturn {\n    json: {\n        paciente_id: pacienteId,\n        token: token,\n        validated: true\n    }\n};"
      },
